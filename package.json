{
  "name": "hello_world_typescript",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "test": "cd src/__test__ && jest --no-cache  --collect-coverage",
    "build": "tsc",
    "start": "npm run build && node dist/source/main.js"
  },
  "dependencies": {},
  "devDependencies": {
    "typescript": "5.3.3",
    "jest": "29.7.0",
    "@types/jest": "29.5.12",
    "ts-jest": "29.1.2"
  },
  "jest": {
    "moduleFileExtensions": ["ts","tsx","js"],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/*Test.(ts|tsx)"
    ]
  }
}
